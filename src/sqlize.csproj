<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<PackageId>sqlize</PackageId>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Version>0.1.0</Version>
		<Authors>DIDIER C.</Authors>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Title>SQLize</Title>
		<PackageProjectUrl>https://github.com/Clemkd/sqlize</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Clemkd/sqlize</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>sqlize;sql;ef;entity;framework;core;generator</PackageTags>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\README.md" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>
